{
  "name": "faktenforum",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "yarn run barrels && tsc --project tsconfig.compile.json",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "yarn run barrels && tsnd --inspect --exit-child --cls --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "yarn run test:lint && yarn run test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "yarn run test:unit",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:generate": "npx prisma generate",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix",
    "prettier": "prettier '**/*.{ts,js,json,md,yml,yaml}' --write"
  },
  "dependencies": {
    "@prisma/client": "^4.12.0",
    "@tsed/ajv": "^7.18.2",
    "@tsed/common": "^7.18.2",
    "@tsed/core": "^7.18.2",
    "@tsed/di": "^7.18.2",
    "@tsed/engines": "^7.18.2",
    "@tsed/exceptions": "^7.18.2",
    "@tsed/json-mapper": "^7.18.2",
    "@tsed/logger": "^6.3.4",
    "@tsed/logger-file": "^6.3.4",
    "@tsed/openspec": "^7.18.2",
    "@tsed/passport": "^7.18.2",
    "@tsed/platform-cache": "^7.18.2",
    "@tsed/platform-exceptions": "^7.18.2",
    "@tsed/platform-express": "^7.18.2",
    "@tsed/platform-log-middleware": "^7.18.2",
    "@tsed/platform-middlewares": "^7.18.2",
    "@tsed/platform-params": "^7.18.2",
    "@tsed/platform-response-filter": "^7.18.2",
    "@tsed/platform-views": "^7.18.2",
    "@tsed/prisma": "^7.18.2",
    "@tsed/schema": "^7.18.2",
    "@tsed/swagger": "^7.18.2",
    "ajv": "^8.12.0",
    "barrelsby": "^2.5.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.2",
    "method-override": "^3.0.0",
    "passport": "^0.6.0"
  },
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "^10.0.2",
    "@tsed/cli-plugin-eslint": "4.4.5",
    "@tsed/cli-plugin-jest": "4.4.5",
    "@tsed/cli-plugin-passport": "4.4.5",
    "@tsed/cli-plugin-prisma": "4.4.5",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.11",
