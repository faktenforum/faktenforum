{
  "name": "faktenforum",
  "version": "0.0.2",
  "description": "",
  "scripts": {
    "build": "npm run barrels && tsc --project tsconfig.compile.json && tsc-alias -p tsconfig.compile.json",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "npm run barrels && tsnd --inspect --exit-child --cls --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:migrate:prod": "prisma migrate deploy && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "prepare:dev": "docker-compose -f docker-compose.local.yml up -d",
    "test": "npm run test:lint && npm run test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "npm run test:unit",
    "prisma": "npm run prisma:generate && npm run prisma:migrate",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:generate": "npx prisma generate",
    "prisma:erd": "npx prisma generate --generator erdPdf erd",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix",
    "prettier": "prettier '**/*.{ts,js,json,md,yml,yaml}' --write",
    "prepare": "husky"
  },
  "dependencies": {
