{
  "name": "faktenforum",
  "version": "0.0.2",
  "description": "",
  "scripts": {
    "build": "npm run barrels && tsc --project tsconfig.compile.json && tsc-alias -p tsconfig.compile.json",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "npm run graphql && npm run barrels && npm run prisma:pull && npm run prisma && tsnd --inspect --exit-child --cls --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "npm run graphql && cross-env NODE_ENV=production node dist/index.js",
    "prepare:dev": "docker-compose -f docker-compose.local.yml up -d",
    "test": "npm run test:lint && npm run test:coverage",
    "test:unit": "cross-env NODE_ENV=test vitest run",
    "test:coverage": "npm run test:unit --coverage",
    "prisma": "npm run prisma:generate",
    "prisma:pull": "npx prisma db pull",
    "prisma:generate": "npx prisma generate",
    "prisma:erd": "npx prisma generate --generator erdPdf erd",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix",
    "prettier": "prettier '**/*.{ts,js,json,md,yml,yaml}' --write",
    "prepare": "if [ \"$NODE_ENV\" != \"production\" ]; then husky install; fi",
    "graphql": "graphql-codegen --config codegen.yml"
  },
