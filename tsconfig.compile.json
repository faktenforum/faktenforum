{
  "extends": "./tsconfig.json",
  "exclude": ["prisma", "minio","node_modules","dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "module": "ES2022",
    "outDir": "./dist",
    "moduleResolution": "bundler",
    "declaration": true,
    "noResolve": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "noEmit": false,
    "emitDeclarationOnly": false,
    "inlineSources": true
  }
}
